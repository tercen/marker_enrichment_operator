{
  "name": "Marker Enrichment Score",
  "description": "Compute a standardised marker enrichment score per cluster.",
  "tags": [
    "flow cytometry",
    "descriptive statistics"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/marker_enrichment_operator"
  ],
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Variable",
            "type": "",
            "description": "Variable identifiers (channel/marker)",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Group",
            "type": "",
            "description": "Group identifiers (cluster)",
            "ontologyMapping": "group",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement values",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}